{
  "main": "code.js",
  "version": "0.0.1",
  "block": {
    "properties": {
      "imageData": {},
      "noise": {
        "settings": {
          "min": 0,
          "max": 30,
          "default": 10
        },
        "type": "slider"
      },
      "subWidth": {
        "type": "number"
      },
      "subHeight": {
        "type": "number"
      },
      "zoom": {
        "settings": {
          "default": 100,
          "min": 50,
          "max": 200
        },
        "type": "slider"
      },
      "colorVariance": {
        "type": "number"
      },
      "invertColors": {
        "type": "checkbox"
      },
      "filterHue": {
        "settings": {
          "max": 360
        },
        "type": "slider"
      },
      "filterStrength": {
        "type": "slider"
      }
    },
    "propertyOrder": [
      "imageData",
      "subWidth",
      "subHeight",
      "noise",
      "zoom",
      "colorVariance",
      "invertColors",
      "filterHue",
      "filterStrength"
    ]
  },
  "name": "lcd-imagedata",
  "description": "Takes an imagedata, and creates an lcd/tv effected output.",
  "tags": [
    "lcd",
    "effect",
    "imageData"
  ]
}
